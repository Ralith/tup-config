ifeq (@(TUP_CONFIG_GCONF),y)

include_rules

CPPFLAGS += $(LIBZCONF_CPPFLAGS)
CPPFLAGS += `@(PKG_CONFIG) --cflags gtk+-2.0 gmodule-2.0 libglade-2.0`
LDFLAGS += `@(PKG_CONFIG) --libs gtk+-2.0 gmodule-2.0 libglade-2.0`
LDFLAGS += $(LIBZCONF_A)
LDFLAGS += -dl

: gconf.c |> !cc |> gconf.o {objs}
: {objs} | $(LIBZCONF_A) |> !link |> tup-gconf

endif
