include_rules

CPPFLAGS += $(LIBZCONF_CPPFLAGS)
CPPFLAGS += `pkg-config pkg-config --cflags gtk+-2.0 gmodule-2.0 libglade-2.0`
CFLAGS += -g
CFLAGS += -O2
CFLAGS += -Wall
LDFLAGS += `pkg-config --libs gtk+-2.0 gmodule-2.0 libglade-2.0`
LDFLAGS += $(LIBZCONF_A)
LDFLAGS += -dl

: gconf.c |> !cc |> gconf.o {objs}
: {objs} | $(LIBZCONF_A) |> !link |> tup-gconf
