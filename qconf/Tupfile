ifeq (@(TUP_CONFIG_QCONF),y)

include_rules

CPPFLAGS += $(LIBZCONF_CPPFLAGS)
CPPFLAGS += `@(PKG_CONFIG) --cflags QtCore QtGui Qt3Support QtXml`
LDFLAGS += `@(PKG_CONFIG) --libs QtCore QtGui Qt3Support QtXml`
LDFLAGS += $(LIBZCONF_A)
LDFLAGS += -ldl

: qconf.h |> !moc |> qconf.moc
: qconf.cc | qconf.moc |> !cxx |> qconf.o {objs}
: {objs} | $(LIBZCONF_A) |> !link |> tup-qconf

endif
